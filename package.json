{
  "name": "better-softphone",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "bcrypt": "^1.0.3",
    "bluebird": "^3.5.0",
    "cooldux": "^0.5.0",
    "hapi": "^16.5.2",
    "hapi-auth-cookie": "^7.0.0",
    "inert": "^4.2.1",
    "isomorphic-fetch": "^2.2.1",
    "joi": "^10.6.0",
    "knex": "^0.13.0",
    "lodash": "^4.17.4",
    "material-ui": "^0.19.1",
    "pg": "^7.3.0",
    "phone-formatter": "0.0.2",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.0.13",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "twilio": "^3.6.5"
  },
  "scripts": {
    "start": "migrate-up && build && start-server",
    "start-dev": "concurrently \"npm run server-dev\" \"npm run client-dev\"",
    "server-dev": "nodemon ./server/index.js",
    "client-dev": "react-scripts start",
    "migrate-create": "knex migrate:make",
    "migrate-up": "knex migrate:latest",
    "migrate-down": "knex migrate:rollback",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "start-server": "node server/index.js",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "concurrently": "^3.5.0",
    "nodemon": "^1.11.0"
  }
}
